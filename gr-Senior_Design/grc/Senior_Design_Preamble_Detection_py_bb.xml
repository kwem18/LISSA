<?xml version="1.0"?>
<block>
  <name>Preamble Detection</name>
  <key>Senior_Design_Preamble_Detection_py_bb</key>
  <category>[Senior_Design]</category>
  <import>import Senior_Design</import>
  <make>Senior_Design.Preamble_Detection_py_bb($Preamble, $Debug)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Preamble</name>
    <key>Preamble</key>
    <type>int</type>
  </param>

  <param>
    <name>Debug</name>
    <key>Debug</key>
    <type>bool</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
